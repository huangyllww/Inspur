[Defines]
  INF_VERSION                    = 0x00010005
  BASE_NAME                      = NctSioDxe
  FILE_GUID                      = 6D85CF0A-4A5F-49b0-BAFD-A90992677FF7
  MODULE_TYPE                    = UEFI_DRIVER
  VERSION_STRING                 = 1.0
  ENTRY_POINT                    = SioDriverEntryPoint

