      text  = STRING_TOKEN(STR_DISCARD_EXIT_AND_RESET),
      text  = STRING_TOKEN(STR_EMPTY),
      flags = INTERACTIVE,
      key   = EXIT_KEY_DISCARD_EXIT_AND_RESET;

    LOGIN_TYPE_ADMIN;
    subtitle text = STRING_TOKEN(STR_EMPTY);
    subtitle text = STRING_TOKEN(STR_DEFAULT_OPTION);

    text
      help   = STRING_TOKEN(LOAD_DEFAULT_HELP),
      text   = STRING_TOKEN(LOAD_DEFAULT_QUESTION),
      flags  = INTERACTIVE,
      key    = EXIT_KEY_DEFAULT;

    text
      help   = STRING_TOKEN(LOAD_USER_DEFAULT_HELP),
      text   = STRING_TOKEN(LOAD_USER_DEFAULT),
      flags  = INTERACTIVE,
      key    = KEY_RESTORE_USER_DEFAULTS_VALUE;

    text
      help   = STRING_TOKEN(SAVE_USER_DEFAULT_HELP),
      text   = STRING_TOKEN(SAVE_USER_DEFAULT),
      flags  = INTERACTIVE,
      key    = KEY_SAVE_USER_DEFAULTS_VALUE;

    subtitle text = STRING_TOKEN(STR_EMPTY);
    subtitle text = STRING_TOKEN(STR_FIRMWARE_UPDATE_OPTION);

    text
      help  = STRING_TOKEN(UPDATE_BIOS),
      text  = STRING_TOKEN(UPDATE_BIOS_HELP),
      flags = INTERACTIVE,
      key   = KEY_BIOS_UPDATE;


    text
      help  = STRING_TOKEN(UPDATE_BIOS_ALL),
      text  = STRING_TOKEN(UPDATE_BIOS_ALL_HELP),
      flags = INTERACTIVE,
      key   = KEY_BIOS_UPDATE_ALL;


    text
      help  = STRING_TOKEN(UPDATE_BIOS_NETWORK),
      text  = STRING_TOKEN(UPDATE_BIOS_NETWORK_HELP),
      flags = INTERACTIVE,
      key   = KEY_BIOS_UPDATE_NETWORK;


    text
      help  = STRING_TOKEN(UPDATE_BIOS_NETWORK_ALL),
      text  = STRING_TOKEN(UPDATE_BIOS_NETWORK_ALL_HELP),
      flags = INTERACTIVE,
      key   = KEY_BIOS_UPDATE_NETWORK_ALL;

    subtitle text = STRING_TOKEN(STR_EMPTY);


  label LABEL_CHOOSE_FOLDER;
  label LABEL_CHOOSE_FOLDER_END;
  END_LOGIN_TYPE;

  label LABEL_CHANGE_LANGUAGE;
  label LABEL_CHANGE_LANGUAGE_END;

  endform;

endformset;
