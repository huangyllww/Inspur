#include "FormsetConfiguration.h"

formset
  guid     = FORMSET_GUID_EXIT,
  title    = STRING_TOKEN(FORMSET_TITLE_EXIT),
  help     = STRING_TOKEN(FORMSET_TITLE_EXIT_HELP),
  classguid = FORMSET_GUID_EXIT,

  LANGUAGE_VALUE_VARSTORE
  SYSTEM_PASSWORD_VARSTORE

  form formid = ROOT_FORM_ID,
    title    = STRING_TOKEN(FORMSET_TITLE_EXIT);

    subtitle text = STRING_TOKEN(STR_EMPTY);

    subtitle text = STRING_TOKEN(STR_SAVE_OPTION);
    text
      help   = STRING_TOKEN(SAVE_CHANGE_EXIT_HELP),
      text   = STRING_TOKEN(SAVE_CHANGE_EXIT_QUESTION),
      flags  = INTERACTIVE,
      key    = EXIT_KEY_SAVE_EXIT;

    text
      help   = STRING_TOKEN(DISCARD_CHANGE_HELP),
      text   = STRING_TOKEN(DISCARD_CHANGE_QUESTION),
      flags  = INTERACTIVE,
      key    = EXIT_KEY_DISCARD;

    subtitle text = STRING_TOKEN(STR_EMPTY);
    text
      help   = STRING_TOKEN(SAVE_CHANGE_HELP),
      text   = STRING_TOKEN(SAVE_CHANGE_QUESTION),
      flags  = INTERACTIVE,
      key    = EXIT_KEY_SAVE;

    text
      help  = STRING_TOKEN(STR_DISCARD_HELP),
      text  = STRING_TOKEN(STR_DISCARD),
      text  = STRING_TOKEN(STR_EMPTY),
      flags = INTERACTIVE,
      key   = EXIT_KEY_DISCARD_MODIFY;

    subtitle text = STRING_TOKEN(STR_EMPTY);

    text
      help  = STRING_TOKEN(STR_SAVE_EXIT_AND_RESET_HELP),
      text  = STRING_TOKEN(STR_SAVE_EXIT_AND_RESET),
      text  = STRING_TOKEN(STR_EMPTY),
      flags = INTERACTIVE,
      key   = EXIT_KEY_SAVE_EXIT_AND_RESET;

    text
